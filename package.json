{
  "name": "torn-base-script",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:safe": "tsc && vite build",
    "build:dev:watch": "vite build --watch --mode dev",
    "serve:dist": "serve dist -l 5174 --cors",
    "dev:tm": "concurrently -k -r \"bun:build:dev:watch\" \"bun:serve:dist\"",
    "biome": "biome check --write",
    "biome:unsafe": "biome check --write --unsafe"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "serve": "^14.2.5",
    "tornapi-typescript": "^5.1.2",
    "vite": "^7.3.0",
    "vite-plugin-monkey": "^7.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "@types/jquery": "^3.5.33",
    "@types/tampermonkey": "^5.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
