{
  "name": "ff-scouter-v3",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:safe": "tsc && vite build",
    "build:dev:watch": "vite build --watch --mode dev",
    "serve:dist": "serve dist -l 5174 --cors",
    "dev:tm": "concurrently -k -r \"bun:build:dev:watch\" \"bun:serve:dist\"",
    "biome": "biome check --write",
    "biome:unsafe": "biome check --write --unsafe",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "idb": "^8.0.3",
    "tornapi-typescript": "^5.1.2",
    "vite": "^7.3.0",
    "vite-plugin-monkey": "^7.1.8",
    "vite-plugin-version-mark": "^0.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "@types/jquery": "^3.5.33",
    "@types/tampermonkey": "^5.0.5",
    "@vitest/coverage-v8": "4.0.16",
    "concurrently": "^9.2.1",
    "jsdom": "^27.4.0",
    "serve": "^14.2.5",
    "vitest": "^4.0.16",
    "vitest-indexeddb": "^0.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
